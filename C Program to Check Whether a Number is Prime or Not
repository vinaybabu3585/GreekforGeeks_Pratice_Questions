#include<stdio.h>
int main()
{
    int num;
    printf("Enter the number: ");
    scanf("%d",&num);
    int is_prime=1;
    for(int i=2;i<num;i++)
    {
        if(num%i==0)
        {
            is_prime=0;
            break;
        }
    }
    if(is_prime)
    {
        printf("%d is prime number",num);
    }
    else
    {
        printf("%d is not prime number",num);
    }
}
